{
  "usesTypeScript": true,
  "usesCssModule": false,
  "testLibrary": "Testing Library",
  "cssPreprocessor": "style.tsx",
  "component": {
    "default": {
      "customTemplates": {
        "component": ".templates/component/TemplateName.tsx",
        "style": ".templates/component/TemplateName.style.tsx",
        "story": ".templates/component/TemplateName.stories.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": ".templates/page/TemplateName.tsx",
        "style": ".templates/page/TemplateName.style.tsx",
        "story": ".templates/page/TemplateName.stories.tsx"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    }
  }
}